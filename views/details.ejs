<div class="container mx-auto px-2 lg:px-5 py-12 ">
  <h1 class="text-3xl font-medium mb-6 text-center">User Details</h1>
  <table class="w-full">
    <thead>
      <tr>
        <th class="py-2 px-4 bg-gray-200">Name</th>
        <th class="py-2 px-4 bg-gray-200">Email</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
        <tr>
          <td class="py-2 px-4 border-b"><%= user.name %></td>
          <td class="py-2 px-4 border-b"><%= user.email %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>